{
  "manifest_version": 2,

  "name": "Rebus",
  "description": "This extention turns a web page into a rebus.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Create rebus"
  },

  "web_accessible_resources": [
    "fonts/*.*",
    "external/js/*.js",
    "external/css/*.css"
 ],
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "background": {
    "scripts": ["trigger.js"]
  },
  "key": ""
}
